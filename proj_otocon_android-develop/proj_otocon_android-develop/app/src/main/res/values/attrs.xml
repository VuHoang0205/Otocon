<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="androidWheelView">
        <attr name="awv_textsize" format="integer" />
        <attr name="awv_lineSpace" format="float" />
        <attr name="awv_centerTextColor" format="integer" />
        <attr name="awv_outerTextColor" format="integer" />
        <attr name="awv_dividerTextColor" format="integer" />
        <attr name="awv_itemsVisibleCount" format="integer" />
        <attr name="awv_isLoop" format="boolean" />
        <attr name="awv_initialPosition" format="integer" />
        <attr name="awv_scaleX" format="float" />
    </declare-styleable>

    <!-- Home Option Toppage -->
    <declare-styleable name="custom_layout_option_toppage">
        <attr name="textTitle" format="string" />
        <attr name="srcImage" format="reference" />
    </declare-styleable>

    <!--Recommend Home View  -->
    <declare-styleable name="layout_recommend_home_view">
        <attr name="custitle" format="string" />
        <attr name="cusimage" format="reference" />
    </declare-styleable>

    <declare-styleable name="RequiredLabelView">
        <attr name="titleRequired" format="string" />
    </declare-styleable>

    <declare-styleable name="TextFieldView">
        <attr name="editTextTitle" format="string" />
        <attr name="editTextTitleRequired" format="string" />
        <attr name="editTextHeight" format="reference" />
        <attr name="android:maxLines" />
        <attr name="android:imeOptions" />
        <attr name="android:inputType" />
        <attr name="android:gravity" />
    </declare-styleable>

    <declare-styleable name="ChooserFieldView">
        <attr name="dropdownTitleRequired" format="string" />
    </declare-styleable>


    <declare-styleable name="EditTextRegisterParty">
        <attr name="editTextRegisterParty" format="string" />
        <attr name="registerPartyRequired" format="string" />
        <attr name="registerPartyHeight" format="reference" />
        <attr name="android:maxLines" />
        <attr name="android:imeOptions" />
        <attr name="android:inputType" />
        <attr name="android:gravity" />
    </declare-styleable>

    <declare-styleable name="DropDownRegisterTextView">
        <attr name="dropdownTitle" format="string" />
    </declare-styleable>

    <declare-styleable name="ConfirmInformationTextView">
        <attr name="android:visibility" />
        <attr name="android:inputType" />
    </declare-styleable>

</resources>